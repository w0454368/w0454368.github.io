<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="150.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="portMan.Controller">
    <children>
        <Label alignment="CENTER" layoutX="21.0" layoutY="24.0" prefHeight="38.0" prefWidth="355.0" text="Are you sure you want to delete this entry?" textAlignment="CENTER">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Button fx:id="promptDel" layoutX="21.0" layoutY="75.0" mnemonicParsing="false" onAction="#confirmDel" prefHeight="53.0" prefWidth="122.0" text="Delete">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <Button fx:id="promptCancel" layoutX="253.0" layoutY="75.0" mnemonicParsing="false" onAction="#cancelDel" prefHeight="53.0" prefWidth="122.0" text="Cancel">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
