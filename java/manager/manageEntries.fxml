            <Font size="36.0" />
         </font>
      </Button>
      <Button layoutX="71.0" layoutY="250.0" mnemonicParsing="false" onAction="#toDeleteEntry" prefHeight="121.0" prefWidth="194.0" text="Delete">
         <font>
            <Font size="36.0" />
         </font>
      </Button>
      <Button layoutX="342.0" layoutY="104.0" mnemonicParsing="false" onAction="#toMoveEntry" prefHeight="121.0" prefWidth="194.0" text="Move">
         <font>
            <Font size="36.0" />
         </font>
      </Button>
      <Button layoutX="342.0" layoutY="250.0" mnemonicParsing="false" onAction="#toStartScene" prefHeight="121.0" prefWidth="194.0" text="Back">
