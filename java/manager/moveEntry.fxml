<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="portMan.Controller">
    <children>
        <Label alignment="CENTER" layoutX="48.0" prefHeight="94.0" prefWidth="500.0" text="Change Entry Category">
            <font>
                <Font size="48.0" />
            </font>
        </Label>
        <ListView fx:id="fileList" layoutX="22.0" layoutY="227.0" prefHeight="163.0" prefWidth="370.0" />
        <Button layoutX="408.0" layoutY="91.0" mnemonicParsing="false" onAction="#moveEntry" prefHeight="139.0" prefWidth="178.0" text="Move">
            <font>
                <Font size="36.0" />
            </font>
        </Button>
        <Button layoutX="408.0" layoutY="249.0" mnemonicParsing="false" onAction="#toManageEntries" prefHeight="139.0" prefWidth="178.0" text="Back">
            <font>
                <Font size="36.0" />
            </font>
        </Button>
        <TextField fx:id="listNameField" layoutX="22.0" layoutY="94.0" prefHeight="54.0" prefWidth="246.0" promptText="Enter file name here">
            <font>
                <Font size="18.0" />
            </font>
        </TextField>
        <Button layoutX="274.0" layoutY="94.0" mnemonicParsing="false" onAction="#searchNameWrap" prefHeight="54.0" prefWidth="119.0" text="Search">
            <font>
                <Font size="24.0" />
            </font>
        </Button>
      <ChoiceBox fx:id="categoryChoice" layoutX="138.0" layoutY="161.0" prefHeight="54.0" prefWidth="254.0" />
      <Label alignment="CENTER" layoutX="22.0" layoutY="161.0" prefHeight="54.0" prefWidth="119.0" text="Move to:">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
