      <Button layoutX="23.0" layoutY="276.0" mnemonicParsing="false" onAction="#toViewEntries" prefHeight="107.0" prefWidth="166.0" text="View Entries">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button layoutX="219.0" layoutY="276.0" mnemonicParsing="false" onAction="#toManageEntries" prefHeight="107.0" prefWidth="166.0" text="Manage Entries">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button layoutX="414.0" layoutY="276.0" mnemonicParsing="false" onAction="#exitProgram" prefHeight="107.0" prefWidth="166.0" text="Exit">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <TextArea fx:id="welcomeText" layoutX="23.0" layoutY="22.0" prefHeight="235.0" prefWidth="555.0">
